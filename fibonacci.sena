inicio

  @@ codigo em sena de fibonacci

  escrevat("Fibonnaci")
  escrevat("") @@ pular linha

  inteiro x

  escrevat("Digite o número")
	leiai(x)
	
  inteiro i << 0
	inteiro a << 0
  inteiro b << 1
  inteiro aux
	
  escrevat("Sequência de Fibonnaci:")
	enquanto(i < x) {
		escrevai(a)
		aux << a
		a << b
		b << b + aux

    i << i + 1
	}

fim